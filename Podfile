  use_frameworks!

  # Network
  pod 'SDWebImage', '~> 4'
  pod 'Apollo', '~> 0.7.0'

  # Geolocation
  pod 'CMMapLauncher', '~> 1.1.0'

  target 'DescartaeTests' do
    inherit! :search_paths
  end

  target 'DescartaeUITests' do
    inherit! :search_paths
  end

end
